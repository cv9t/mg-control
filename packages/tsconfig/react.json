{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "display": "React",
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "module": "ESNext",
    "allowJs": true,
    "declaration": false,
    "declarationMap": false,
    "incremental": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "jsx": "react-jsx"
  }
}
