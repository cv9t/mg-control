{
  "extends": "./base.json",
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React",
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "es2015",
    "lib": ["es2020", "dom"],
    "module": "esnext",
    "allowJs": true,
    "declaration": false,
    "declarationMap": false,
    "incremental": true,
    "noEmit": true,
    "resolveJsonModule": true
  }
}
